@{
    Layout = null;
}
<table class="table table-striped table-condensed">
    <thead>
        <tr>
            <th width="30%">Thời gian</th>
            <th width="30%">Trạng thái</th>
            <th width="20%">Vị trí</th>
            <th width="20%">Ghi chú</th>
        </tr>
    </thead>
    <tbody>
        @if (ViewBag.JourneyList.Length > 0)
        {
            foreach (dynamic c in ViewBag.JourneyList)
            {
                <tr>
                    <td width="30%">@DateTime.Parse(c.DateCreate.ToString()).AddHours(7).ToString("dd/MM/yyyy HH:mm:ss")</td>
                    <td width="30%">@c.Status</td>
                    <td width="20%">@c.Location</td>
                    <td width="20%">@c.Note</td>
                </tr>
            }
        }
    </tbody>
</table>
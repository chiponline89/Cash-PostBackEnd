@model dynamic
<div class="row">
    <div class="form-horizontal" role="form">
        <div class="form-group">
            <label class="col-sm-3 control-label">Mã vận đơn/yêu cầu</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="shipping_getback_lading_code" value="@Model.lading_code" placeholder="Mã vận đơn/yêu cầu">
            </div>
            <div class="col-sm-5">
                <input type="text" class="form-control" id="shipping_getback_customer_code" value="@Model.customer_code" readonly="readonly" placeholder="Mã Đại lý">
            </div>
        </div>
        <div class="form-group">
            <label for="inputPassword3" class="col-sm-3 control-label">Tên đại lý</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="shipping_getback_customer_name" value="@Model.customer_name" placeholder="Họ và tên Khách hàng">
            </div>
            <label class="col-sm-2 control-label">Điện thoại</label>
            <div class="col-sm-3">
                <input type="text" id="shipping_getback_customer_mobile" placeholder="Điện thoại" value="@Model.customer_mobile" class="form-control" />
            </div>
        </div>
        <div class="form-group">
            <label for="inputPassword3" class="col-sm-3 control-label">Họ tên Người nhận</label>
            <div class="col-sm-4">
                <input type="text" class="form-control" id="shipping_getback_receiver_name" value="@Model.receiver_name" placeholder="Họ và tên Người nhận">
            </div>
            <label class="col-sm-2 control-label">Điện thoại</label>
            <div class="col-sm-3">
                <input type="text" id="shipping_getback__receiver_mobile" placeholder="Đthoại" value="@Model.receiver_mobile" class="form-control" />
            </div>
        </div>
        <div class="form-group">
            <label for="inputPassword3" class="col-sm-3 control-label">Địa chỉ</label>
            <div class="col-sm-9">
                <input type="text" class="form-control" id="shipping_getback_receiver_address" value="@Model.receiver_address" placeholder="Địa chỉ giao hàng">
            </div>
            </div>
        </div>
</div>
<script>
    function createShippingGetbackRequest() {
        $.ajax({
            url: "/ServiceRequest/Request/CreateShippingGetbackRequest",
            dataType: "json",
            data: {
                lading_code: $('#shipping_getback_lading_code').val(),
                customer_code: $('#shipping_getback_customer_code').val(),
                customer_name: $('#shipping_getback_customer_name').val(),
                customer_mobile: $('#shipping_getback_customer_mobile').val(),
                receiver_name: $('#shipping_getback_receiver_name').val(),
                receiver_mobile: $('#shipping_getback_receiver_mobile').val(),
                receiver_address: $('#shipping_getback_receiver_address').val(),
                notes:$('#notes').val()
            },
            success: function (data) {
                alert(data.response_message);
                return;
            }
        });
    }
</script>